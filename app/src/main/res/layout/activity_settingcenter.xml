<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:pktzj="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="org.pktzj.mobilesafe.activity.SettingCenter"
    android:orientation="vertical">

    <TextView
        style="@style/tv_title"
        android:text="设置中心" />
    <org.pktzj.mobilesafe.view.SettingCenterItemView
        pktzj:title="自动更新设置"
        pktzj:content="自动更新已经关闭-自动更新已经打开"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </org.pktzj.mobilesafe.view.SettingCenterItemView>
    <org.pktzj.mobilesafe.view.SettingCenterItemView
        pktzj:title="黑名单拦截设置"
        pktzj:content="黑名单拦截设置已经关闭-黑名单拦截设置已经打开"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </org.pktzj.mobilesafe.view.SettingCenterItemView>
</LinearLayout>
